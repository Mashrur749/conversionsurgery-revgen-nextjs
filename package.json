{
	"name": "conversionsurgery-revgen-nextjs",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"cf:build": "npx @opennextjs/cloudflare build",
		"cf:dev": "npx wrangler dev",
		"cf:deploy": "npx @opennextjs/cloudflare build && npx wrangler deploy",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"db:check": "drizzle-kit check",
		"db:seed": "npx tsx scripts/seed.ts",
		"db:setup": "npm run db:migrate && npm run db:seed",
		"db:reset": "npx drizzle-kit drop && npm run db:setup"
	},
	"dependencies": {
		"@auth/drizzle-adapter": "^1.11.1",
		"@aws-sdk/client-s3": "^3.985.0",
		"@aws-sdk/s3-request-presigner": "^3.985.0",
		"@neondatabase/serverless": "^1.0.2",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"drizzle-orm": "^0.45.1",
		"googleapis": "^171.4.0",
		"libphonenumber-js": "^1.12.36",
		"lucide-react": "^0.563.0",
		"next": "16.1.5",
		"next-auth": "^4.24.13",
		"openai": "^6.18.0",
		"radix-ui": "^1.4.3",
		"react": "19.1.5",
		"react-dom": "19.1.5",
		"resend": "^6.9.1",
		"sharp": "^0.34.5",
		"sonner": "^2.0.7",
		"stripe": "^20.3.1",
		"tailwind-merge": "^3.4.0",
		"twilio": "^5.12.1",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@opennextjs/cloudflare": "^1.16.3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20.19.31",
		"@types/react": "^19.1.5",
		"@types/react-dom": "^19.1.5",
		"dotenv": "^17.2.4",
		"drizzle-kit": "^0.31.8",
		"eslint": "^9",
		"eslint-config-next": "15.4.6",
		"shadcn": "^3.8.4",
		"tailwindcss": "^4",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.7.4",
		"wrangler": "^4.63.0"
	}
}
